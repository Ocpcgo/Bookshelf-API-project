{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-undef": "off",
        "indent": "off",
        "linebreak-style": "off",
        "eol-last": "off",
        "space-before-blocks":"off",
        "no-trailing-spaces":"off",
        "object-curly-spacing":"off",
        "semi": "off",
        "quotes": "off",
        "comma-dangle": "off",
        "max-len":"off"
    }
}
